               <Button fx:id="hiddeneventButton" layoutX="250.0" layoutY="223.0" mnemonicParsing="false" prefHeight="58.0" prefWidth="128.0" text="히든이벤트">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Button>
               <Button fx:id="specialButton" layoutX="406.0" layoutY="223.0" mnemonicParsing="false" prefHeight="59.0" prefWidth="126.0" text="특수이벤트">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Button>
               <CheckBox fx:id="hiddeneventCheckBox" layoutX="261.0" layoutY="289.0" mnemonicParsing="false" text=" On/ Off" />
               <CheckBox fx:id="specialeventCheckBox" layoutX="413.0" layoutY="289.0" mnemonicParsing="false" text="On/ Off" />
            </children></AnchorPane>
        </Scene>
    </scene>
</Stage>
