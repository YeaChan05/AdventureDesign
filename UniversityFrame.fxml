<?import javafx.stage.Stage?>

<Stage xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UniversityFrameController">
    <scene>
        <Scene>
            <AnchorPane prefHeight="400.0" prefWidth="600.0">
            <children>
               <TextField fx:id="universityTextField" alignment="CENTER" layoutX="201.0" layoutY="14.0" prefHeight="48.0" prefWidth="198.0" text="학교">
                  <font>
                     <Font size="30.0" />
                  </font>
               </TextField>
               <Button fx:id="studyButton" layoutX="244.0" layoutY="91.0" mnemonicParsing="false" onAction="#studyEvent" prefHeight="54.0" prefWidth="94.0" text="공부 ">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Button>
               <Button fx:id="clubactivitiesButton" layoutX="405.0" layoutY="91.0" mnemonicParsing="false" prefHeight="53.0" prefWidth="89.0" text="동아리">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Button>
               <ImageView fx:id="returnIamgeView" fitHeight="58.0" fitWidth="70.0" layoutX="528.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../../OneDrive/바탕%20화면/return.png" />
                  </image>
               </ImageView>
               <ListView fx:id="lecutreListView" layoutX="57.0" layoutY="164.0" prefHeight="211.0" prefWidth="85.0" />
