               <Label fx:id="lecutreLabel" alignment="CENTER" layoutX="60.0" layoutY="91.0" prefHeight="55.0" prefWidth="78.0" style="-fx-border-color: black;" text="강의">
                  <font>
                     <Font size="20.0" />
                  </font>
               </Label>
               <Button fx:id="majorlecture2Button" layoutX="57.0" layoutY="199.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="83.0" text="전공 2" />
               <Button fx:id="majorlecture3Button" layoutX="58.0" layoutY="234.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="82.0" text="전공 3" />
               <Button fx:id="majorlecture4Button" layoutX="57.0" layoutY="270.0" mnemonicParsing="false" prefHeight="37.0" prefWidth="83.0" text="전공 4" />
               <Button fx:id="culturelecture1Button" layoutX="57.0" layoutY="307.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="84.0" text="교양 1" />
